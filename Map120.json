rs":["",0,0,2]},{"code":401,"indent":0,"parameters":["大妖狐"]},{"code":401,"indent":0,"parameters":["「我完全使用了魔法，所以没问题。"]},{"code":401,"indent":0,"parameters":["但是，如果感觉不舒服就告诉我哦？"]},{"code":401,"indent":0,"parameters":["如果无法说话，就举起一只手」"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["清彦"]},{"code":401,"indent":0,"parameters":["「明白」"]},{"code":205,"indent":0,"parameters":[1,{"list":[{"code":16,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":16,"indent":null}]},{"code":205,"indent":0,"parameters":[2,{"list":[{"code":3,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":3,"indent":null}]},{"code":205,"indent":0,"parameters":[1,{"list":[{"code":18,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":18,"indent":null}]},{"code":213,"indent":0,"parameters":[1,3,true]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["大妖狐"]},{"code":401,"indent":0,"parameters":["「好～♪"]},{"code":401,"indent":0,"parameters":["那么，开始换衣服吧♪」"]},{"code":213,"indent":0,"parameters":[-1,1,false]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["大妖狐握住他的手，靠近他帮忙。"]},{"code":401,"indent":0,"parameters":["\\!这样下去的话，理性可能无法承受。"]},{"code":401,"indent":0,"parameters":["他本身对女性的抵抗力并不是完全没有，"]},{"code":401,"indent":0,"parameters":["但也有限度。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["清彦"]},{"code":401,"indent":0,"parameters":["「没、没关系！"]},{"code":401,"indent":0,"parameters":["我一个人也可以……如果有什么困难会告诉你」"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["大妖狐"]},{"code":401,"indent":0,"parameters":["「是吗？"]},{"code":401,"indent":0,"parameters":["嗯♪那就试穿一下吧」"]},{"code":205,"indent":0,"parameters":[-1,{"list":[{"code":16,"indent":null},